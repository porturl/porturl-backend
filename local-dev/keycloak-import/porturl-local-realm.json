{
  "realm": "porturl-local",
  "enabled": true,
  "ssoSessionIdleTimeout": 15552000,
  "ssoSessionMaxLifespan": 15552000,
  "offlineSessionIdleTimeout": 15552000,
  "offlineSessionMaxLifespan": 15552000,
  "revokeRefreshToken": true,
  "refreshTokenMaxReuse": 1,
  "roles": {
    "realm": [
      {
        "name": "ROLE_ADMIN",
        "description": "Administrator role"
      }
    ]
  },
  "identityProviders": [
    {
      "alias": "demo-idp",
      "displayName": "Demo Realm Login",
      "providerId": "keycloak-oidc",
      "enabled": true,
      "trustEmail": true,
      "storeToken": false,
      "addReadTokenRoleOnCreate": false,
      "authenticateByDefault": false,
      "linkOnly": false,
      "firstBrokerLoginFlowAlias": "first broker login",
      "config": {
        "clientAuthMethod": "client_secret_post",
        "clientId": "demo-broker",
        "clientSecret": "demo-broker-secret",
        "tokenUrl": "https://localhost:8443/realms/demo/protocol/openid-connect/token",
        "authorizationUrl": "https://localhost:8443/realms/demo/protocol/openid-connect/auth",
        "logoutUrl": "https://localhost:8443/realms/demo/protocol/openid-connect/logout",
        "userInfoUrl": "https://localhost:8443/realms/demo/protocol/openid-connect/userinfo",
        "issuer": "https://localhost:8443/realms/demo",
        "syncMode": "IMPORT",
        "useJwksUrl": "true"
      }
    }
  ],
  "identityProviderMappers": [
    {
      "name": "username-importer",
      "identityProviderAlias": "demo-idp",
      "identityProviderMapper": "oidc-username-idp-mapper",
      "config": {
        "template": "${CLAIM.preferred_username}",
        "target": "LOCAL"
      }
    }
  ],
  "authenticatorConfig": [
    {
      "alias": "idp-redirect-config",
      "config": {
        "defaultProvider": "demo-idp"
      }
    }
  ],
  "authenticationFlows": [
    {
      "alias": "browser-with-redirect",
      "description": "browser based authentication with automatic idp redirect",
      "providerId": "basic-flow",
      "topLevel": true,
      "builtIn": false,
      "authenticationExecutions": [
        {
          "authenticator": "auth-cookie",
          "requirement": "ALTERNATIVE",
          "priority": 10,
          "userSetupAllowed": false,
          "authenticatorFlow": false
        },
        {
          "authenticator": "identity-provider-redirector",
          "requirement": "ALTERNATIVE",
          "priority": 25,
          "authenticatorConfig": "idp-redirect-config",
          "userSetupAllowed": false,
          "authenticatorFlow": false
        },
        {
          "authenticator": "auth-spnego",
          "requirement": "DISABLED",
          "priority": 30,
          "userSetupAllowed": false,
          "authenticatorFlow": false
        },
        {
          "requirement": "ALTERNATIVE",
          "priority": 30,
          "flowAlias": "forms",
          "userSetupAllowed": false,
          "authenticatorFlow": true
        }
      ]
    },
    {
      "alias": "forms",
      "description": "Username, password, otp and other form based authenticator",
      "providerId": "basic-flow",
      "topLevel": false,
      "builtIn": false,
      "authenticationExecutions": [
        {
          "authenticator": "auth-username-password-form",
          "requirement": "REQUIRED",
          "priority": 10,
          "userSetupAllowed": false,
          "authenticatorFlow": false
        },
        {
          "authenticator": "auth-otp-form",
          "requirement": "OPTIONAL",
          "priority": 20,
          "userSetupAllowed": false,
          "authenticatorFlow": false
        }
      ]
    }
  ],
  "browserFlow": "browser-with-redirect",
  "users": [
    {
      "username": "admin",
      "enabled": true,
      "email": "admin@example.com",
      "firstName": "Admin",
      "lastName": "User",
      "emailVerified": true,
      "credentials": [
        {
          "type": "password",
          "value": "admin",
          "temporary": false
        }
      ],
      "realmRoles": [
        "ROLE_ADMIN",
        "offline_access",
        "uma_authorization"
      ],
      "requiredActions": []
    },
    {
      "username": "service-account-porturl-management-client",
      "enabled": true,
      "serviceAccountClientId": "porturl-management-client",
      "realmRoles": [
        "ROLE_ADMIN"
      ],
      "clientRoles": {
        "realm-management": [
          "realm-admin"
        ]
      }
    }
  ],
  "clients": [
    {
      "clientId": "porturl-management-client",
      "name": "PortUrl Local Management",
      "enabled": true,
      "secret": "local-management-secret",
      "serviceAccountsEnabled": true,
      "directAccessGrantsEnabled": false,
      "standardFlowEnabled": false,
      "authorizationServicesEnabled": false,
      "publicClient": false
    },
    {
      "clientId": "porturl-android",
      "name": "PortUrl Android App",
      "enabled": true,
      "publicClient": true,
      "redirectUris": [
        "org.friesoft.porturl:/*",
        "http://localhost:*"
      ],
      "webOrigins": [
        "*"
      ],
      "standardFlowEnabled": true,
      "directAccessGrantsEnabled": true,
      "attributes": {
        "pkce.code.challenge.method": "S256"
      }
    },
    {
      "clientId": "porturl-backend-swagger-client",
      "name": "Swagger UI",
      "enabled": true,
      "publicClient": true,
      "redirectUris": [
        "http://localhost:8080/swagger-ui/oauth2-redirect.html"
      ],
      "standardFlowEnabled": true,
      "directAccessGrantsEnabled": true,
      "attributes": {
        "pkce.code.challenge.method": "S256"
      }
    }
  ]
}
